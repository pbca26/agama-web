    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.24.1",
    "bitgo-utxo-lib": "git://github.com/pbca26/bitgo-utxo-lib",
    "bluebird": "^3.5.0",
    "file-loader": "^3.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "qrcode.react": "^0.8.0",
    "rc-slider": "8.5.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-double-scrollbar": "git://github.com/pbca26/react-double-scrollbar",
    "react-hot-loader": "^1.3.0",
